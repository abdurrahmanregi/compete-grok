# API key for xAI models (required for Grok API calls)
XAI_API_KEY=your_xai_grok_api_key_here
# API key for Tavily search tool (required for web search functionality)
TAVILY_API_KEY=your_tavily_key_here
# API key for Mistral models (optional, used for PDF conversion)
MISTRAL_API_KEY=your_mistral_key_here
# API key for Linkup search tool (required for deep web analysis)
LINKUP_API_KEY=your_linkup_key_here
# API key for LangChain (optional, for tracing and project management)
LANGCHAIN_API_KEY=enter_key
# Enable LangChain tracing (set to true for debugging)
LANGCHAIN_TRACING_V2=true
# LangChain project name (for organizing traces)
LANGCHAIN_PROJECT=compete-grok
# Set to any value to enable DEBUG logging level (default is INFO)
VERBOSE=